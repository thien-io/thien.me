    "typescript": "^4.5.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=1 npm-run-all --parallel type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "yarn run lint"
    ],
    "*.+(js|json|ts|tsx)": [
      "yarn run format"
    ]
